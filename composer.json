{
  "name": "team64j/laravel-manager",
  "description": "Laravel manager",
  "homepage": "https://github.com/team64j/laravel-manager",
  "keywords": [
    "laravel",
    "manager"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Serg Kochin"
    }
  ],
  "require": {
    "php": ">=8.1",
    "tymon/jwt-auth": "^2.0",
    "team64j/laravel-evolution": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Team64j\\LaravelManager\\": "src"
    }
  },
  "scripts": {
    "post-package-install": [
      "@php artisan jwt:secret",
      "@php artisan vendor:publish --provider=\"Team64j\\LaravelManager\\Providers\\ManagerServiceProvider\""
    ],
    "pre-package-update": [
      "@php artisan jwt:secret",
      "@php artisan vendor:publish --provider=\"Team64j\\LaravelManager\\Providers\\ManagerServiceProvider\""
    ]
  }
}
